{
  "name": "rhoml-kubernetes",
  "version": "0.1.0",
  "author": "Rhommel Lamas",
  "summary": "Puppet module to bootstrap & manage kubernetes cluster",
  "license": "MIT",
  "source": "https://github.com/rhoml/rhoml-kubernetes",
  "project_page": "https://github.com/rhoml/rhoml-kubernetes",
  "issues_url": "https://github.com/rhoml/rhoml-kubernetes/issues",
  "dependencies": [
    {"name":"puppetlabs-stdlib","version_requirement":">= 4.1.0"},
    {"name":"puppetlabs/apt","version_requirement":">= 1.8.0 <= 3.0.0"}
  ],
  "data_provider": null
  "operatingsystem_support": [
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7.8",
        "8.0"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.4.0"
    }
  ]
}

